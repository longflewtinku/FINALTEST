<?xml version="1.0" encoding="gb2312"?>
<Schema>
    <!-- Groups Definition -->
    <Groups>
        <Group>
            <ID>Override_Config</ID>
            <Title>OverrideConfig</Title>
            <Order>1</Order>
            <Description></Description>
        </Group>
    </Groups>
    <!-- Files Definition -->
    <Files>
        <File>
            <ID>sys_F2</ID>
            <FileName>overrideparams.xml</FileName>
            <Description></Description>
        </File>
    </Files>
    <!-- Parameter Definition -->
    <Parameters>
        <Header>
            <Title>Currency Config</Title>
            <DisplayStyle>foldable</DisplayStyle>
            <DefaultStyle>open</DefaultStyle>
            <Display>true</Display>


            <!-- Currency Code -->
            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <PID>currencyCode</PID>
                <Title>Currency Code</Title>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Select>{"ZAR":"ZAR","USD":"USD","GBP":"GBP"}</Select>
                <Defaultvalue>GBP</Defaultvalue>
                <Description>Currency Code</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <!-- Currency Number -->
            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>currencyNum</PID>
                <Title>Currency Num</Title>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>036</Defaultvalue>
                <Description>Currency Number</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>


            <!-- Country Number -->
            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>countryNum</PID>
                <Title>Country Number</Title>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>036</Defaultvalue>
                <Description>Country Number</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <DataType>String</DataType>
                <PID>language</PID>
                <Title>Language Selection</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Select>{"en_GB":"en_GB","is_IS":"is_IS"}</Select>
                <Defaultvalue>en_GB</Defaultvalue>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

        </Header> <!--Region -  Currency Config -->
        <Header>
            <Title>Transaction Configuration</Title>
            <DisplayStyle>foldable</DisplayStyle>
            <DefaultStyle>open</DefaultStyle>
            <Display>true</Display>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <DataType>String</DataType>
                <PID>mailOrder</PID>
                <Title>Mail Order</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>Y</Defaultvalue>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <DataType>String</DataType>
                <PID>telephone</PID>
                <Title>Telephone Supported</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>Y</Defaultvalue>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <DataType>String</DataType>
                <PID>preauth</PID>
                <Title>PreAuth Supported</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>Y</Defaultvalue>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <DataType>String</DataType>
                <PID>cashback</PID>
                <Title>Cashback Supported</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>N</Defaultvalue>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <DataType>String</DataType>
                <PID>refund</PID>
                <Title>Refund Supported</Title>
                <Length>1</Length>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>Y</Defaultvalue>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <PID>tipAllowed</PID>
                <Title>Tip Allowed</Title>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>Y</Defaultvalue>
                <Description>Support TIP</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <PID>custRefRequired</PID>
                <Title>Reference Required</Title>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Select>{"OPTIONAL":"OPTIONAL","MANDATORY":"MANDATORY", "DISABLED":"DISABLED"}</Select>
                <Defaultvalue>DISABLED</Defaultvalue>
                <Description>Reference Required</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <DataType>String</DataType>
                <PID>emvSupported</PID>
                <Title>EMV Supported</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>Y</Defaultvalue>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <DataType>String</DataType>
                <PID>cardholderPresent</PID>
                <Title>Cardholder Present</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>Y</Defaultvalue>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <DataType>String</DataType>
                <PID>contactlessSupported</PID>
                <Title>CTLS Supported</Title>
                <Length>1</Length>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>Y</Defaultvalue>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <PID>loyaltySupported</PID>
                <Title>Loyalty Supported</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>N</Defaultvalue>
                <Display>true</Display>
                <Description>Loyalty Integration</Description>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

        </Header> <!--Region -  Currency Config -->
        <Header>
            <Title>Payment Switch Configuration</Title>
            <DisplayStyle>foldable</DisplayStyle>
            <DefaultStyle>open</DefaultStyle>
            <Display>true</Display>


            <!-- Host IP -->

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <PID>paymentSwitch_useSsl</PID>
                <Title>SSL Enabled</Title>
                <Required>true</Required>
                <Readonly>true</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>Y</Defaultvalue>
                <Description>Use SSL Comms</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>paymentSwitch_ip_host</PID>
                <Title>Host Primary</Title>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>https://staging.integrate.demo.com/v1/token/</Defaultvalue>
                <Description>Primary Host URL</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>paymentSwitch_ip_host2nd</PID>
                <Title>Host Secondary</Title>
                <Required>true</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>https://staging.integrate.demo.com/v1/token/</Defaultvalue>
                <Description>Secondary Host URL</Description>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>


            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>paymentSwitch_certificateFile</PID>
                <Title>Certificate File</Title>
                <Required>true</Required>
                <Readonly>true</Readonly>
                <Defaultvalue>ps.pem</Defaultvalue>
                <Description>SSL Certificate File</Description>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>paymentSwitch_privateKeyFile</PID>
                <Title>Private Key File</Title>
                <Required>false</Required>
                <Readonly>true</Readonly>
                <Defaultvalue></Defaultvalue>
                <Description>SSL Client Private Key</Description>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>paymentSwitch_privateKeyPassword</PID>
                <Title>Private Key Password</Title>
                <Required>false</Required>
                <Readonly>true</Readonly>
                <Defaultvalue></Defaultvalue>
                <Description>SSL Client Private Key Password</Description>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>paymentSwitch_privateKeyCertificate</PID>
                <Title>SSL Client Private Key Certificate File</Title>
                <Required>false</Required>
                <Readonly>true</Readonly>
                <Defaultvalue></Defaultvalue>
                <Description>SSL Client Private Key Cert</Description>
                <Display>false</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

        </Header> <!--Region -  Payment Switch -->

        <Header>
            <Title>GoInsight Server Configuration</Title>
            <DisplayStyle>foldable</DisplayStyle>
            <DefaultStyle>open</DefaultStyle>
            <Display>true</Display>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <PID>paxstoreUpload</PID>
                <Title>GoInsight Upload Transactions</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>N</Defaultvalue>
                <Description>Upload transaction data to Paxstore and GoInsight</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>
        </Header>

        <Header>
            <Title>Email Server Configuration</Title>
            <DisplayStyle>foldable</DisplayStyle>
            <DefaultStyle>open</DefaultStyle>
            <Display>true</Display>

            <Parameter>
                <Type>single</Type>
                <InputType>select</InputType>
                <PID>mailMerchant</PID>
                <Title>EMail Merchant</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Select>{"Y":"Y","N":"N"}</Select>
                <Defaultvalue>N</Defaultvalue>
                <Description>Email Merchant</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>mailStoreName</PID>
                <Title>Merchant Store Name</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue></Defaultvalue>
                <Description>Merchant store name for the receipt</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>mailMerchantAddress</PID>
                <Title>Merchant Address</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>positiveterminals@gmail.com</Defaultvalue>
                <Description>Merchant email address</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>


            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>mailHost</PID>
                <Title>Email Host (Server)</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>smtp.gmail.com</Defaultvalue>
                <Description>Email server to use for receipts</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>mailPort</PID>
                <Title>Email Server port to use</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>465</Defaultvalue>
                <Description>Port on the email server</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>mailUser</PID>
                <Title>Email Server User</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>positiveterminals@gmail.com</Defaultvalue>
                <Description>User on the email server</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>mailPassword</PID>
                <Title>Email Server Password</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>3ftsolution5</Defaultvalue>
                <Description>Password on the email server</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>


            <Parameter>
                <Type>single</Type>
                <InputType>text</InputType>
                <PID>mailSender</PID>
                <Title>Email Sender</Title>
                <Required>false</Required>
                <Readonly>false</Readonly>
                <Defaultvalue>positiveterminals@gmail.com</Defaultvalue>
                <Description>Email "from" address</Description>
                <Display>true</Display>
                <GroupID>Override_Config</GroupID>
                <FileID>sys_F2</FileID>
            </Parameter>

        </Header>

    </Parameters>
</Schema>


