<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="uiHeadlineFont">
        <item name="android:textSize">25sp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorHeader</item>
        <item name="android:textFontWeight">700</item>
    </style>

    <style name="uiMenuTitleFont">
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="uiMenuHeaderFont">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/intermedium</item>
        <item name="android:textColor">#000000</item>
